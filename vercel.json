{
    "buildCommand": "npm install && npm install --prefix client && npm run build --prefix client",
    "outputDirectory": "client/dist",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index.js"
        }
    ],
    "functions": {
        "api/index.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}