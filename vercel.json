{
    "buildCommand": "npm run build",
    "outputDirectory": "client/dist",
    "devCommand": "npm run dev",
    "installCommand": "npm install",
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/index.js"
        }
    ],
    "functions": {
        "api/index.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    }
}